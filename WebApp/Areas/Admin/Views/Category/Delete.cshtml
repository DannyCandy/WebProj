@model WebApp.Models.Category

<h2>Are you sure you want to delete this?</h2>

<div>   
    <h4 class="mt-2">Name: @Model.NameCategory</h4>
</div>

<form asp-action="Delete" method="post" class="mt-2">
    <input type="hidden" asp-for="Idcategory" />
    <input type="submit" value="Delete" class="btn btn-danger" /> |
    <a asp-action="Index" class="btn btn-success">Cancel</a>

</form>
@section Scripts {
}