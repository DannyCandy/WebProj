/* Slide customer scripts: */ 

document.addEventListener("DOMContentLoaded", function () {
        const swiper = document.querySelector(
          "swiper-container.mySwiper-customer"
        ).swiper;

        const boxDots = document.querySelector("ul#box-slide-customer-dots");

        const lengthDots = new Array(swiper.slides.length).fill(0);
        const handleCreateElement = (index) =>
          `<li class="bullet_customer ${index === 0 ? "active" : ""}"></li>`;

        const listDots = lengthDots.reduce(
          (acc, element, index) => acc + handleCreateElement(index),
          ``
        );
        boxDots.innerHTML = listDots;

        swiper.on("slideChange", function () {
          const listDotsNewS = document.querySelectorAll(
            "ul#box-slide-customer-dots li.bullet_customer"
          );
          listDotsNewS.forEach((e, index) => {
            if (e.classList.contains("active")) {
              e.classList.remove("active");
            }
            if (index === swiper.realIndex) {
              e.classList.add("active");
            }
          });
        });
        const btnNext = document.querySelector(
          ".box-slide-customer-oh .btn-next-customer"
        );
        btnNext.addEventListener("click", () => {
          swiper.slideNext();
        });
        const btnPrev = document.querySelector(
          ".box-slide-customer-oh .btn-prev-customer"
        );
        btnPrev.addEventListener("click", () => {
          swiper.slidePrev();
        });
      });
