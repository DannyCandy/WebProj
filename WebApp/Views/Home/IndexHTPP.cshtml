<script>
	(function ($) {

		$(document).ready(function () {
			if (window.location.href?.includes('/en/')) {
				document.querySelectorAll("#ongtt_city option")[0].innerText = "Province";
				document.querySelectorAll("#ongtt_district option")[0].innerText = "District";
			}


			var ajaxUrl = 'https://hanuti.vn/wp-admin/admin-ajax.php';
			$('#ongtt_city').change(function (event) {
				var city_id = $(this).val();
				var district_id = '';
				$.ajax({
					url: ajaxUrl,
					type: 'POST',
					dataType: 'html',
					data: {
						action: 'showChild',
						city_id: city_id
					},
				})
					.done(function (data) {
						$('select#ongtt_district').html(data);
					})
					.fail(function () {
						console.log("error");
					})
					.always(function () {
					});
				get_address(city_id, district_id);
			});

			$(document).on('change', '#ongtt_district', function () {
				var district_id = $(this).val();
				var city_id = $('#ongtt_city').val();
				get_address(city_id, district_id);
			});

			$(document).on('click', '.localstore_box', function () {
				$('.localstore_box').removeClass('active');
				$(this).addClass('active');
				var data_map = $(this).attr('data-map');
				$("#lc_map iframe").attr('src', data_map);
			});



		});

		function first_map() {
			let first_map = $('.localstore_box.active').attr('data-map');
			$("#lc_map iframe").attr('src', first_map);
		}

		function get_address(city_id, district_id) {
			var ajaxUrl = 'https://hanuti.vn/wp-admin/admin-ajax.php';
			$.ajax({
				url: ajaxUrl,
				type: 'POST',
				dataType: 'json',
				data: {
					action: 'showAddress',
					city_id: city_id,
					district_id: district_id,

				},
				beforeSend: function () {
					$('.map-show').addClass('loading');
				},
				success: function (response) {
					if (response.success) {
						$('.localstore_search_results').empty().append($(response.data));
						first_map();
						$('.map-show').removeClass('loading');
					} else {
						$("map-show").removeClass("loading");
						$(".map-show").append("<p>Bản đồ đang được cập nhật!</p>");
					}

				},
				error: function (jqXHR, textStatus, errorThrown) {
					console.log('The following error occured: ' + textStatus, errorThrown);
				},
			})

		}

	})(jQuery);
</script>

