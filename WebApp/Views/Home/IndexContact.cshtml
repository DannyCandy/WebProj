        window.addEventListener("scroll", () => {
          if (window.innerWidth >= 1024) {
            if (listCategoryRes.classList.contains("active")) {
              listCategoryRes.classList.remove("active");
            }
            if (window.scrollY > 500) {
              nav.classList.add("active");
