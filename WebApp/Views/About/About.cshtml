                  localStorage.setItem("indexTab", JSON.stringify(0));
                }
                const listPage = document.querySelector("nav .list_page");
                const listMenuNav = document.querySelector(
                  "aside#menu_nav .container_list .list_menu_nav"
                );
                const boxSelectNav = document.querySelector(
                  "aside#menu_nav .select_menu_nav .container_select"
                );
                const handleToggleClass = (index) => {
                  if (index === 0) {
                    listPage.classList.add("customer");
                    listPage.classList.remove("partner");
                    listPage.classList.remove("buyer");
                    btnChangeCategory.classList.add("customer");
                    btnChangeCategory.classList.remove("partner");
                    btnChangeCategory.classList.remove("buyer");
                    listMenuNav.classList.add("customer");
                    listMenuNav.classList.remove("partner");
                    listMenuNav.classList.remove("buyer");
                    boxSelectNav.classList.add("customer");
                    boxSelectNav.classList.remove("partner");
                    boxSelectNav.classList.remove("buyer");
                  }
                  if (index === 1) {
                    listPage.classList.remove("customer");
                    listPage.classList.add("partner");
                    listPage.classList.remove("buyer");
                    btnChangeCategory.classList.remove("customer");
                    btnChangeCategory.classList.add("partner");
                    btnChangeCategory.classList.remove("buyer");
                    listMenuNav.classList.remove("customer");
                    listMenuNav.classList.add("partner");
                    listMenuNav.classList.remove("buyer");
                    boxSelectNav.classList.remove("customer");
                    boxSelectNav.classList.add("partner");
                    boxSelectNav.classList.remove("buyer");
                  }
                  if (index === 2) {
                    listPage.classList.remove("customer");
                    listPage.classList.remove("partner");
                    listPage.classList.add("buyer");
                    btnChangeCategory.classList.remove("customer");
                    btnChangeCategory.classList.remove("partner");
                    btnChangeCategory.classList.add("buyer");
                    listMenuNav.classList.remove("customer");
                    listMenuNav.classList.remove("partner");
                    listMenuNav.classList.add("buyer");
                    boxSelectNav.classList.remove("customer");
                    boxSelectNav.classList.remove("partner");
                    boxSelectNav.classList.add("buyer");
                  }
                };
                handleToggleClass(Number(JSON.parse(localStorage.getItem("indexTab"))));
        
                const handleChangeIndexTab = () => {
                  listTab.forEach((e, index) => {
                    if (
                      index === Number(JSON.parse(localStorage.getItem("indexTab")))
                    ) {
                      e.classList.add("active");
                      listCategoryResLi[index].classList.add("active");
                    } else {
                      e.classList.remove("active");
                      listCategoryResLi[index].classList.remove("active");
                    }
                  });
                };
                if (window.innerWidth < 1024) {
                  listWait.forEach((e, index) => {
                    e.addEventListener("click", () => {
                      localStorage.setItem("isSelect", JSON.stringify(true));
                      localStorage.setItem("indexTab", JSON.stringify(index));
                      document.querySelector("body").style.overflow = "auto";
                      sectionWait.style.display = "none";
                      handleChangeIndexTab();
                      handleToggleClass(Number(index));
                    });
                  });
                  if (!JSON.parse(localStorage.getItem("isSelect"))) {
                    sectionWait.style.display = "flex";
                    document.querySelector("body").style.overflow = "hidden";
                  }
                }
        
                listTab.forEach((e, index) => {
                  if (index === Number(JSON.parse(localStorage.getItem("indexTab")))) {
                    e.classList.add("active");
                    listCategoryResLi[index].classList.add("active");
                  } else {
                    e.classList.remove("active");
                    listCategoryResLi[index].classList.remove("active");
                  }
        
                  e.addEventListener("click", () => {
                    localStorage.setItem("indexTab", JSON.stringify(index));
                    handleChangeIndexTab();
                    handleToggleClass(Number(index));
                  });
        
                  listCategoryResLi[index].addEventListener("click", () => {
                    localStorage.setItem("indexTab", JSON.stringify(index));
                    handleChangeIndexTab();
                    handleToggleClass(Number(index));
                    aside.classList.add("active");
                  });
                });
        
                // add event click btn menu change height nav
                const btnMenuRes = document.querySelector("nav .box_res .btn_menu_res");
          
        
                btnMenuRes.addEventListener("click", () => {
                  if (window.innerWidth >= 1024) {
                    nav.classList.toggle("active");
                  } else {
                    if (!aside.classList.contains("active")) {
                      document.querySelector("body").style.overflow = "hidden";
                    }
                    aside.classList.toggle("active");
                  }
                });
                const btnCloseAside = document.querySelector(
                  "aside#menu_nav button.btn_close"
                );
        
                btnCloseAside.addEventListener("click", () => {
                  aside.classList.remove("active");
                });
                  if(window.innerWidth <1023){
                  const listMenuNav = document.querySelectorAll(".container_list .list_menu_nav li");
                  listMenuNav.forEach((e, index) => {
                    e.addEventListener("click", () => {
                      scrollToCheck = true;
                        setTimeout(() => {
                          scrollToCheck = false;
                      }, 1000)
                      aside.classList.remove("active");
                    });
                  });
                }
        
        /* ScrollToTop scripts: */ 
        
        document.addEventListener("DOMContentLoaded", function () {
          gsap.registerPlugin(ScrollTrigger);
          gsap.registerPlugin(ScrollToPlugin)
          document
            .getElementById("btn-scroll-to-top")
            .addEventListener("click", () => {
            scrollToCheck = true;
            gsap.to(window, {duration: 1, scrollTo:{y:0, offsetY:0}});
            setTimeout(() => {
              scrollToCheck = false;
            }, 1000)
          });
          gsap.to("#btn-scroll-to-top rect", {
            scrollTrigger: {
              trigger: "body",
              scrub: true,
              start: "top top",
              end: ()=>"+=" + document.body.offsetHeight,
            },
            strokeDashoffset: "0",
          });
        });
        </script>
